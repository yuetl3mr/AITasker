mixin alert-success(time)
    - if (messages.success)
        .message.info
            div(class="alert alert-success" show-alert data-time=time) #{messages.success} 


mixin alert-error(time)
    - if (messages.danger)
        .message.info
            div(class="alert alert-danger" show-alert data-time=time) #{messages.danger} 